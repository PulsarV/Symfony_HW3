{% extends '::base.html.twig' %}

{% block title %}
    {{ parent() }} - {{ team.name }}  national football team
{% endblock %}
{% block header %}{{ team.name }} national football team{% endblock %}
{% block content %}
    {{ team.info }}
    <p>
        <p><a class="btn btn-default" href="{{ playersRoute }}{{ team.name }}" role="button">Team players</a></p>
        <p><a class="btn btn-default" href="{{ coachsRoute }}{{ team.name }}" role="button">Team coachs</a></p>
    </p>
{% endblock %}