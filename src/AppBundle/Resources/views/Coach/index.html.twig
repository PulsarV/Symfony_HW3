{% extends '::base.html.twig' %}

{% block title %}
    {{ parent() }} - {{ coachs[0].team }} national football team coachs
{% endblock %}
{% block header %}
    {{ coachs[0].team }} national football team coachs
{% endblock %}
{% block content %}
    {% for coach in coachs %}
        <a class="btn btn-default col-lg-3" href="{{ path('coachView', {'teamName': coach.team, 'coachName':coach.name}) }}" role="button">{{ coach.name }}</a>
    {% endfor %}
{% endblock %}